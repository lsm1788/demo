				System.out.println("15");
				break;
			case 3:
				System.out.println("16");
				break;
			case 4:
				System.out.println("16");
				break;
			case 5:
				System.out.println("17");
				break;
			case 6:
				System.out.println("17");
				break;
			case 7:
				System.out.println("18");
				break;
			case 8:
				System.out.println("18");
				break;
			case 9:
				System.out.println("19");
				break;
			case 10:
				System.out.println("19");
				break;
			case 11:
				System.out.println("20");
				break;
			case 12:
				System.out.println("20");
				break;
			case 13:
				System.out.println("21");
				break;
			case 14:
				System.out.println("21");
				break;
			case 15:
				System.out.println("22");
				break;
			case 16:
				System.out.println("22");
				break;
			case 17:
				System.out.println("23");
